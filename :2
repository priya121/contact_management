require 'random_data'

def generator 
   contacts_list = {:first_name => Random.firstname, :last_name => Random.lastname, :dob => Random.date, :address => Random.address_line_1, :postcode => Random.uk_post_code}
end

def list_generator
    list = (1..100).to_a
    list.each do |contact|
        puts generator.to_json
        puts Random.date 
    end
end
